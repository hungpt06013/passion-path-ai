{
  "version": 2,
  "buildCommand": "",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/api/(.*)", "destination": "/api",
      "destination": "/main.html"
    }
  ]
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
